{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "azuredata-factory-training"
		},
		"AzureBlobStorageblobGitHub_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorageblobGitHub'"
		},
		"AzureDatabricks1_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'AzureDatabricks1'"
		},
		"AzureSqlDatabaseSqlserver_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabaseSqlserver'"
		},
		"ls_azuredataLakestorage_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_azuredataLakestorage'"
		},
		"ls_zure_blobsorage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_zure_blobsorage'"
		},
		"ls_azuredataLakestorage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://azureg2storageaccount.dfs.core.windows.net"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "azuredata-factory-training_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorageblobGitHub_connectionString": {
						"value": "[parameters('AzureBlobStorageblobGitHub_connectionString')]"
					},
					"AzureDatabricks1_accessToken": {
						"value": "[parameters('AzureDatabricks1_accessToken')]"
					},
					"AzureSqlDatabaseSqlserver_connectionString": {
						"value": "[parameters('AzureSqlDatabaseSqlserver_connectionString')]"
					},
					"ls_azuredataLakestorage_accountKey": {
						"value": "[parameters('ls_azuredataLakestorage_accountKey')]"
					},
					"ls_zure_blobsorage_connectionString": {
						"value": "[parameters('ls_zure_blobsorage_connectionString')]"
					},
					"ls_azuredataLakestorage_properties_typeProperties_url": {
						"value": "[parameters('ls_azuredataLakestorage_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}